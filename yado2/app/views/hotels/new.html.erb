<%= form_for @hotel, url: {action: :create} do |f| %>
    <%= f.label :hname,'ホテル名'%>
    <%= f.text_field :hname%>
    <%= f.label :place,'県'%>
    <%= f.text_field :place%>
    <%= f.label :price,'価格'%>
    <%= f.text_field :price%>
    <%= f.label :sptype,'温泉の特徴'%>
    <%= f.text_field :sptype %>
    <%= f.label :comment,'紹介文'%>
    <%= f.text_field :comment%>
    <%= f.label :img, 'イメージ'%>
    <%= f.file_field :img %>
    <%= f.submit"登録"%>
    
<% end %>